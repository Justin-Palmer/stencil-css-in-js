{
  "name": "stencil-css-in-js",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-css-in-js/stencil-css-in-js.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress:update-images": "cypress run --env updateSnapshots=true",
    "cypress:suppress-image-failure": "cypress run --env failOnSnapshotDiff=false",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "1.2.0",
    "@stencil/state-tunnel": "1.0.1",
    "cypress": "3.8.2",
    "cypress-image-snapshot": "3.1.1",
    "cypress-shadow-dom": "1.3.0"
  }
}
